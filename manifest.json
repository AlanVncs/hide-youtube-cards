{
    "manifest_version": 2,
    "name": "Hide Youtube Cards",
    "version": "1.0.0",
    "description": "Remove the annoying cards at the end of the video",
    "content_scripts": [
        {
            "js": [
                "main.js"
            ],
            "matches": [
                "https://www.youtube.com/watch*"
            ]
        }
    ]
}